{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      search: '',\n      dialogVisible2: false,\n      currentPage: 1,\n      pageSize: 2,\n      total: 0,\n      params: {\n        date: ''\n      },\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    load() {\n      request.get(\"http://localhost:9090/tch/allStuMsg\", {\n        params: {\n          classes: this.user.classes\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    query() {\n      request.get(\"http://localhost:9090/tch/query\", {\n        params: {\n          name: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    query2() {\n      request.get(\"http://localhost:9090/stu/queryStuDateAcidMsg\", {\n        params: {\n          stuId: this.user.stuId,\n          date: this.params.date\n        }\n      }).then(res => {\n        console.log(res);\n        console.log(\"res.data.length\", res.data.length);\n        this.tableData = res.data; //this.total=res.data.length\n      });\n    },\n\n    dateFormat(picker) {\n      this.params.date = picker;\n    },\n\n    formatDate(row, column) {\n      // 获取单元格数据\n      let data = row[column.property];\n\n      if (data == null) {\n        return null;\n      }\n\n      let dt = new Date(data);\n      return dt.getFullYear() + '-' + (dt.getMonth() + 1 < 10 ? '0' + (dt.getMonth() + 1) : dt.getMonth() + 1) + '-' + (dt.getDate() < 10 ? '0' + dt.getDate() : dt.getDate()) + ' ' + (dt.getHours() < 10 ? '0' + dt.getHours() : dt.getHours()) + ':' + (dt.getMinutes() < 10 ? '0' + dt.getMinutes() : dt.getMinutes()) + ':' + (dt.getSeconds() < 10 ? '0' + dt.getSeconds() : dt.getSeconds());\n    },\n\n    handleCheck(id) {\n      request.put(\"http://localhost:9090/tch/check/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"核验成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表单\n      });\n    },\n\n    handleEdit2() {\n      this.dialogVisible2 = true;\n      this.form = {};\n      this.$refs['upload'].clearFiles(); //清除历史文件列表\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,MAAM,EAAC,EAHF;MAILC,cAAc,EAAC,KAJV;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,CANL;MAOLC,KAAK,EAAC,CAPD;MAQLC,MAAM,EAAE;QACNC,IAAI,EAAE;MADA,CARH;MAWLC,SAAS,EAAC;IAXL,CAAP;EAiBD,CAxBY;;EAyBb;EACAC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKb,IAAL,GAAYc,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CA9BY;;EA+BbC,OAAO,EAAE;IACPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKlB,IAAL,CAAUqB,GAAV,GAAgBH,GAAG,CAACpB,IAApB;IACD,CAJM;;IAKPiB,IAAI,GAAE;MACJpB,OAAO,CAAC2B,GAAR,CAAY,qCAAZ,EAAkD;QAChDhB,MAAM,EAAC;UACLiB,OAAO,EAAC,KAAKxB,IAAL,CAAUwB;QADb;MADyC,CAAlD,EAIGC,IAJH,CAIQN,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAeU,GAAG,CAACpB,IAAnB;QACA,KAAKO,KAAL,GAAWa,GAAG,CAACpB,IAAJ,CAAS2B,MAApB;MACD,CARD;IASD,CAfM;;IAgBPC,KAAK,GAAE;MACL/