B;MACA,KAAKF,IAAL,GAAY,EAAZ;MACA,KAAKoD,KAAL,CAAW,QAAX,EAAqBC,UAArB,GAHW,CAGyB;IACrC,CApFM;;IAqFPC,gBAAgB,GAAE;MAAQ;MACxB,KAAKvC,IAAL;IACD,CAvFM;;IAwFPwC,mBAAmB,GAAE;MAAK;MACxB,KAAKxC,IAAL;IACD;;EA1FM;AA/BI,CAAf","names":["request","name","components","data","user","form","search","dialogVisible2","currentPage","pageSize","total","params","date","tableData","created","userStr","sessionStorage","getItem","JSON","parse","load","methods","filesUploadSuccess","res","console","log","pic","get","classes","then","length","query","query2","stuId","dateFormat","picker","formatDate","row","column","property","dt","Date","getFullYear","getMonth","getDate","getHours","getMinutes","getSeconds","handleCheck","id","put","code","$message","type","message","msg","handleEdit2","$refs","clearFiles","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["D:\\Desktop\\DZY\\vue\\src\\views\\TchInfo.vue"],"sourcesContent":["<template>\r\n  <div style=\"padding: 10px\">\r\n    <!--功能区域-->\r\n    <div style=\"margin: 10px 0\">\r\n      教师检查学生页面\r\n      <el-button size=\"mini\"  @click=\"handleEdit2()\">上传核酸</el-button>\r\n    </div>\r\n    <!--搜索区域-->\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input v-model=\"search\" placeholder=\"输入关键字\" style=\"width:20%\" clearable/>\r\n<!--      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"query\">查询</el-button>-->\r\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"query2\">日期+学号查询</el-button>\r\n    </div>\r\n    <el-date-picker\r\n        v-model=\"params.date\"\r\n        type=\"date\"\r\n        value-format=\"YYYY-MM-DD\"\r\n        placeholder=\"日期\"\r\n        @change=\"dateFormat\">\r\n    </el-date-picker>\r\n    <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"query2\">日期+学号查询</el-button>\r\n\r\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\r\n      <el-table-column\r\n          prop=\"stuId\"\r\n          label=\"工号\"\r\n          width=\"0\" />\r\n      <el-table-column\r\n          prop=\"name\"\r\n          label=\"姓名\"\r\n          width=\"0\"/>\r\n      <el-table-column\r\n          label=\"核酸图片\">\r\n        <template #default=\"scope\">\r\n          <el-image\r\n              style=\"width: 100px; height: 100px\"\r\n              :src=\"scope.row.pic\"\r\n              :preview-src-list=\"[scope.row.pic]\">\r\n          </el-image>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"time\"\r\n          :formatter=\"formatDate\"\r\n          label=\"上传时间\"\r\n          width=\"0\"/>\r\n      <el-table-column\r\n          prop=\"check\"\r\n          label=\"核验\"\r\n          width=\"0\"/>\r\n      <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          width=\"200px\">\r\n\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\"  @click=\"handleCheck(scope.row.id)\">核验</el-button>\r\n        </template>\r\n\r\n      </el-table-column>\r\n      <el-dialog v-model=\"dialogVisible2\" title=\"添加\" width=\"30%\">\r\n        <el-form :model=\"form\" label-width=\"120px\">\r\n          <el-form-item label=\"学号\">\r\n            <el-input v-model=\"this.user.stuId\" style=\"width: 80%\"/>\r\n          </el-form-item>\r\n          <el-form-item label=\"姓名\">\r\n            <el-input v-model=\"this.user.name\" style=\"width: 80%\"/>\r\n          </el-form-item>\r\n          <el-form-item label=\"图片\">\r\n            <el-upload ref=\"upload\" action=\"http://localhost:9090/files/upload\" :on-success=\"filesUploadSuccess\">\r\n              <el-button type=\"primary\">点击上传</el-button>\r\n            </el-upload>\r\n          </el-form-item>\r\n\r\n\r\n        </el-form>\r\n        <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"add\">确定</el-button>\r\n      </span>\r\n        </template>\r\n      </el-dialog>\r\n    </el-table>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-pagination\r\n          v-model:currentPage=\"currentPage\"\r\n          v-model:page-size=\"pageSize\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n      />\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n\r\n  name: 'HomeView',\r\n  components: {\r\n\r\n  },\r\n  data(){\r\n    return {\r\n      user:{},\r\n      form:{},\r\n      search:'',\r\n      dialogVisible2:false,\r\n      currentPage: 1,\r\n      pageSize: 2,\r\n      total:0,\r\n      params: {\r\n        date: '',\r\n      },\r\n      tableData:[\r\n\r\n\r\n      ],\r\n\r\n    }\r\n  },\r\n  //页面加载时调用\r\n  created() {\r\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(userStr)\r\n    this.load()\r\n  },\r\n  methods: {\r\n    filesUploadSuccess(res) {\r\n      console.log(res)\r\n      this.form.pic = res.data\r\n    },\r\n    load(){\r\n      request.get(\"http://localhost:9090/tch/allStuMsg\",{\r\n        params:{\r\n          classes:this.user.classes,\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data\r\n        this.total=res.data.length\r\n      })\r\n    },\r\n    query(){\r\n      request.get(\"http://localhost:9090/tch/query\",{\r\n        params:{\r\n          name:this.search,\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data\r\n        this.total=res.data.length\r\n      })\r\n    },\r\n    query2(){\r\n      request.get(\"http://localhost:9090/stu/queryStuDateAcidMsg\",{\r\n        params:{\r\n          stuId:this.user.stuId,\r\n          date:this.params.date,\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        console.log(\"res.data.length\",res.data.length)\r\n        this.tableData=res.data\r\n        //this.total=res.data.length\r\n      })\r\n    },\r\n    dateFormat(picker){\r\n      this.params.date=picker\r\n    },\r\n    formatDate(row, column) {\r\n      // 获取单元格数据\r\n      let data = row[column.property]\r\n      if(data == null) {\r\n        return null\r\n      }\r\n      let dt = new Date(data)\r\n      return dt.getFullYear() + '-' +   ((dt.getMonth() + 1) < 10 ? ('0' + (dt.getMonth() + 1)) : (dt.getMonth() + 1)) + '-' +\r\n\r\n          (dt.getDate() < 10 ? ('0' + dt.getDate()) : dt.getDate()) + ' ' +\r\n\r\n          (dt.getHours() < 10 ? ('0' + dt.getHours()) : dt.getHours()) + ':' +\r\n\r\n          (dt.getMinutes() < 10 ? ('0' + dt.getMinutes()) : dt.getMinutes()) + ':' +\r\n\r\n          (dt.getSeconds() < 10 ? ('0' + dt.getSeconds()) : dt.getSeconds())\r\n    },\r\n    handleCheck(id){\r\n\r\n      request.put(\"http://localhost:9090/tch/check/\" + id).then(res=>{\r\n        console.log(res)\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"核验成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n        this.load()  //刷新表单\r\n      })\r\n\r\n\r\n    },\r\n    handleEdit2(){\r\n      this.dialogVisible2=true\r\n      this.form = {}\r\n      this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n    },\r\n    handleSizeChange(){       //改变当前每页的个数\r\n      this.load()\r\n    },\r\n    handleCurrentChange(){    //改变当前页面\r\n      this.load()\r\n    },\r\n  }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}