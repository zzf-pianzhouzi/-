MACA,KAAKtC,IAAL;IACF;;EAhIO;AAvBI,CAAf","names":["E","request","editor","name","components","data","user","loading","form","dialogVisible","search","currentPage","pageSize","total","tableData","vis","detail","created","userStr","sessionStorage","getItem","JSON","parse","load","methods","details","row","filesUploadSuccess","res","console","log","cover","get","then","length","add","$nextTick","config","uploadImgServer","uploadFileName","create","txt","html","save","content","id","put","code","$message","type","message","msg","author","stuId","post","handleEdit","stringify","handleDelete","delete","handleSizeChange","handleCurrentChange","pageNum"],"sourceRoot":"","sources":["D:\\Desktop\\javaee大作业\\vue\\src\\views\\News.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n    <!--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\" v-if=\"user.role===3\">新增</el-button>\n    </div>\n\n    <!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column\n          prop=\"id\"\n          label=\"ID\"\n          sortable\n      >\n      </el-table-column>\n      <el-table-column\n          prop=\"title\"\n          label=\"标题\">\n      </el-table-column>\n      <el-table-column\n          prop=\"author\"\n          label=\"作者\">\n      </el-table-column>\n      <el-table-column\n          prop=\"time\"\n          label=\"时间\">\n      </el-table-column>\n      <el-table-column label=\"操作\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"details(scope.row)\">详情</el-button>\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\" v-if=\"user.role===3\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\" v-if=\"user.role===3\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"提示\" v-model=\"dialogVisible\" width=\"50%\">\n      <el-form :model=\"form\" label-width=\"120px\">\n        <el-form-item label=\"标题\">\n          <el-input v-model=\"form.title\" style=\"width: 50%\"></el-input>\n        </el-form-item>\n\n        <div id=\"div1\"></div>\n<!--                <el-form-item label=\"内容\">\n                  <el-input v-model=\"form.price\" style=\"width: 80%\"></el-input>\n                </el-form-item>-->\n      </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n    <el-dialog title=\"详情\" v-model=\"vis\" width=\"50%\">\n      <el-card>\n        <div v-html=\"detail.content\" style=\"min-height: 100px\"></div>\n      </el-card>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport E from 'wangeditor'\nimport request from \"@/utils/request\";\n\nlet editor;\n\nexport default {\n  name: 'News',\n  components: {},\n  data() {\n    return {\n      user:{},\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      vis: false,\n      detail: {},\n    }\n  },\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\"\n    this.user = JSON.parse(userStr)\n    this.load()\n  },\n  methods: {\n    details(row) {\n      this.detail = row\n      this.vis = true\n    },\n    filesUploadSuccess(res) {\n      console.log(res)\n      this.form.cover = res.data\n    },\n    load() {\n      this.loading = true\n      request.get(\"http://localhost:9090/news\").then(res => {\n        this.loading = false\n        this.tableData = res.data\n        this.total = res.data.length\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n\n      this.$nextTick(() => {\n        // 关联弹窗里面的div，new一个 editor对象\n        if (!editor) {\n          editor = new E('#div1')\n\n          // 配置 server 接口地址\n          editor.config.uploadImgServer = 'http://localhost:9090/files/editor/upload'\n          editor.config.uploadFileName = \"file\"  // 设置上传参数名称\n          editor.create()\n        }\n\n        editor.txt.html(\"\")\n\n      })\n\n    },\n    save() {\n\n\n      this.form.content = editor.txt.html() // 获取 编辑器里面的值，然后赋予到实体当中\n        console.log(this.form.content)\n      if (this.form.id) {  // 更新\n        request.put(\"http://localhost:9090/news\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      } else {  // 新增\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\"\n        let user = JSON.parse(userStr)\n        this.form.author = user.stuId\n        console.log(user)\n        console.log(this.form.author)\n        request.post(\"http://localhost:9090/news\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n\n      this.$nextTick(() => {\n        // 关联弹窗里面的div，new一个 editor对象\n        // 关联弹窗里面的div，new一个 editor对象\n        if (!editor) {\n          editor = new E('#div1')\n\n          // 配置 server 接口地址\n          editor.config.uploadImgServer = 'http://localhost:9090/files/editor/upload'\n          editor.config.uploadFileName = \"file\"  // 设置上传参数名称\n          editor.create()\n        }\n\n        editor.txt.html(row.content)\n      })\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(\"http://localhost:9090/news/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}