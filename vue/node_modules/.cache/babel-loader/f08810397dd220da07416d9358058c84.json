{"ast":null,"code":"import E from 'wangeditor';\nimport request from \"@/utils/request\";\nlet editor;\nexport default {\n  name: 'News',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      vis: false,\n      detail: {}\n    };\n  },\n\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    details(row) {\n      this.detail = row;\n      this.vis = true;\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.cover = res.data;\n    },\n\n    load() {\n      this.loading = true;\n      request.get(\"http://localhost:9090/news\").then(res => {\n        this.loading = false;\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n      this.$nextTick(() => {\n        // 关联弹窗里面的div，new一个 editor对象\n        if (!editor) {\n          editor = new E('#div1'); // 配置 server 接口地址\n\n          editor.config.uploadImgServer = 'http://localhost:9090/files/editor/upload';\n          editor.config.uploadFileName = \"file\"; // 设置上传参数名称\n\n          editor.create();\n        }\n\n        editor.txt.html(\"\");\n      });\n    },\n\n    save() {\n      this.form.content = editor.txt.html(); // 获取 编辑器里面的值，然后赋予到实体当中\n\n      console.log(this.form.content);\n\n      if (this.form.id) {\n        // 更新\n        request.put(\"http://localhost:9090/news\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      } else {\n        // 新增\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n        let user = JSON.parse(userStr);\n        this.form.author = user.stuId;\n        console.log(user);\n        console.log(this.form.author);\n        request.post(\"http://localhost:9090/news\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      this.$nextTick(() => {\n        // 关联弹窗里面的div，new一个 editor对象\n        // 关联弹窗里面的div，new一个 editor对象\n        if (!editor) {\n          editor = new E('#div1'); // 配置 server 接口地址\n\n          editor.config.uploadImgServer = 'http://localhost:9090/files/editor/upload';\n          editor.config.uploadFileName = \"file\"; // 设置上传参数名称\n\n          editor.create();\n        }\n\n        editor.txt.html(row.content);\n      });\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/news/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); // 删除之后重新加载表格的数据\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      // 改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2FA,OAAOA,CAAP,MAAc,YAAd;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,IAAIC,MAAJ;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,OAAO,EAAE,IAFJ;MAGLC,IAAI,EAAE,EAHD;MAILC,aAAa,EAAE,KAJV;MAKLC,MAAM,EAAE,EALH;MAMLC,WAAW,EAAE,CANR;MAOLC,QAAQ,EAAE,EAPL;MAQLC,KAAK,EAAE,CARF;MASLC,SAAS,EAAE,EATN;MAULC,GAAG,EAAE,KAVA;MAWLC,MAAM,EAAE;IAXH,CAAP;EAaD,CAjBY;;EAkBbC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKd,IAAL,GAAYe,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CAtBY;;EAuBbC,OAAO,EAAE;IACPC,OAAO,CAACC,GAAD,EAAM;MACX,KAAKV,MAAL,GAAcU,GAAd;MACA,KAAKX,GAAL,GAAW,IAAX;IACD,CAJM;;IAKPY,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKpB,IAAL,CAAUuB,KAAV,GAAkBH,GAAG,CAACvB,IAAtB;IACD,CARM;;IASPkB,IAAI,GAAG;MACL,KAAKhB,OAAL,GAAe,IAAf;MACAN,OAAO,CAAC+B,GAAR,CAAY,4BAAZ,EAA0CC,IAA1C,CAA+CL,GAAE,IAAK;QACpD,KAAKrB,OAAL,GAAe,KAAf;QACA,KAAKO,SAAL,GAAiBc,GAAG,CAACvB,IAArB;QACA,KAAKQ,KAAL,GAAae,GAAG,CAACvB,IAAJ,CAAS6B,MAAtB;MACD,CAJD;IAKD,CAhBM;;IAiBPC,GAAG,GAAG;MACJ,KAAK1B,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;MAEA,KAAK4B,SAAL,CAAe,MAAM;QACnB;QACA,IAAI,CAAClC,MAAL,EAAa;UACXA,MAAK,GAAI,IAAIF,CAAJ,CAAM,OAAN,CAAT,CADW,CAGX;;UACAE,MAAM,CAACmC,MAAP,CAAcC,eAAd,GAAgC,2CAAhC;UACApC,MAAM,CAACmC,MAAP,CAAcE,cAAd,GAA+B,MAA/B,CALW,CAK4B;;UACvCrC,MAAM,CAACsC,MAAP;QACF;;QAEAtC,MAAM,CAACuC,GAAP,CAAWC,IAAX,CAAgB,EAAhB;MAED,CAbD;IAeD,CApCM;;IAqCPC,IAAI,GAAG;MAGL,KAAKnC,IAAL,CAAUoC,OAAV,GAAoB1C,MAAM,CAACuC,GAAP,CAAWC,IAAX,EAApB,CAHK,CAGiC;;MACpCb,OAAO,CAACC,GAAR,CAAY,KAAKtB,IAAL,CAAUoC,OAAtB;;MACF,IAAI,KAAKpC,IAAL,CAAUqC,EAAd,EAAkB;QAAG;QACnB5C,OAAO,CAAC6C,GAAR,CAAY,4BAAZ,EAA0C,KAAKtC,IAA/C,EAAqDyB,IAArD,CAA0DL,GAAE,IAAK;UAC/DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAIA,GAAG,CAACmB,IAAJ,KAAa,GAAjB,EAAsB;YACpB,KAAKC,QAAL,CA