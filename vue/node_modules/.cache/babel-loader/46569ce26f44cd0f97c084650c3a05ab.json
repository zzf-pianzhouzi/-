{"ast":null,"code":"import E from 'wangeditor';\nimport request from \"@/utils/request\";\nlet editor;\nexport default {\n  name: 'News',\n  components: {},\n\n  data() {\n    return {\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      vis: false,\n      detail: {}\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    details(row) {\n      this.detail = row;\n      this.vis = true;\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.cover = res.data;\n    },\n\n    load() {\n      this.loading = true;\n      request.get(\"http://localhost:9090/news\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false;\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n      this.$nextTick(() => {\n        // 关联弹窗里面的div，new一个 editor对象\n        if (!editor) {\n          editor = new E('#div1'); // 配置 server 接口地址\n\n          /*      editor.config.uploadImgServer = 'http://' + window.server.filesUploadUrl + ':9090/files/editor/upload'\n                editor.config.uploadFileName = \"file\"  // 设置上传参数名称*/\n\n          editor.create();\n        }\n        /*  editor.txt.html(\"\")*/\n\n      });\n    },\n\n    save() {\n      this.form.content = editor.getHtml(); // 获取 编辑器里面的值，然后赋予到实体当中\n\n      if (this.form.id) {\n        // 更新\n        request.put(\"http://localhost:9090/news\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      } else {\n        // 新增\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n        let user = JSON.parse(userStr);\n        this.form.author = user.nickName;\n        request.post(\"http://localhost:9090/news\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      this.$nextTick(() => {\n        // 关联弹窗里面的div，new一个 editor对象\n        // 关联弹窗里面的div，new一个 editor对象\n        if (!editor) {\n          editor = new E('#div1'); // 配置 server 接口地址\n\n          /*          editor.config.uploadImgServer = 'http://localhost:9090/files/editor/upload'\n                    editor.config.uploadFileName = \"file\"  // 设置上传参数名称*/\n\n          editor.create();\n        }\n        /*        editor.txt.html(row.content)*/\n\n      });\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/news/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); // 删除之后重新加载表格的数据\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      // 改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2FA,OAAOA,CAAP,MAAc,YAAd;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,IAAIC,MAAJ;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IADJ;MAELC,IAAI,EAAE,EAFD;MAGLC,aAAa,EAAE,KAHV;MAILC,MAAM,EAAE,EAJH;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,EANL;MAOLC,KAAK,EAAE,CAPF;MAQLC,SAAS,EAAE,EARN;MASLC,GAAG,EAAE,KATA;MAULC,MAAM,EAAE;IAVH,CAAP;EAYD,CAhBY;;EAiBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAnBY;;EAoBbC,OAAO,EAAE;IACPC,OAAO,CAACC,GAAD,EAAM;MACX,KAAKL,MAAL,GAAcK,GAAd;MACA,KAAKN,GAAL,GAAW,IAAX;IACD,CAJM;;IAKPO,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKf,IAAL,CAAUkB,KAAV,GAAkBH,GAAG,CAACjB,IAAtB;IACD,CARM;;IASPY,IAAI,GAAG;MACL,KAAKX,OAAL,GAAe,IAAf;MACAL,OAAO,CAACyB,GAAR,CAAY,4BAAZ,EAA0C;QACxCC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKlB,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADgC,CAA1C,EAMGoB,IANH,CAMQP,GAAE,IAAK;QACb,KAAKhB,OAAL,GAAe,KAAf;QACA,KAAKO,SAAL,GAAiBS,GAAG,CAACjB,IAAJ,CAASyB,OAA1B;QACA,KAAKlB,KAAL,GAAaU,GAAG,CAACjB,IAAJ,CAASO,KAAtB;MACD,CAVD;IAWD,CAtBM;;IAuBPmB,GAAG,GAAG;MACJ,KAAKvB,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;MAEA,KAAKyB,SAAL,CAAe,MAAM;QACnB;QACA,IAAI,CAAC9B,MAAL,EAAa;UACXA,MAAK,GAAI,IAAIF,CAAJ,CAAM,OAAN,CAAT,CADW,CAGX;;UACN;;;UAEME,MAAM,CAAC+B,MAAP;QACF;QAEF;;MAEC,CAbD;IAeD,CA1CM;;IA2CPC,IAAI,GAAG;MACL,KAAK3B,IAAL,CAAU4B,OAAV,GAAoBjC,MAAM,CAACkC,OAAP,EAApB,CADK,CACgC;;MAErC,IAAI,KAAK7B,IAAL,CAAU8B,EAAd,EAAkB;QAAG;QACnBpC,OAAO,CAACqC,GAAR,CAAY,4BAAZ,EAA0C,KAAK/B,IAA/C,EAAqDsB,IAArD,CAA0DP,GAAE,IAAK;UAC/DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAIA,GAAG,CAACiB,IAAJ,KAAa,GAAjB,EAAsB;YACpB,KAAKC,QAAL,CAAc;cACZC,IAAI,EAAE,SADM;cAEZC,OAAO,EAAE;YAFG,CAAd;UAIF,CALA,MAKO;YACL,KAAKF,QAAL,CAAc;cACZC,IAAI,EAAE,OADM;cAEZC,OAAO,EAAEpB,GAAG,CAACqB;YAFD,CAAd;UAIF;;UACA,KAAK1B,IAAL,GAb+D,CAanD;;UACZ,KAAKT,aAAL,GAAqB,KAArB