{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'Permission',\n  components: {},\n\n  data() {\n    return {\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      this.loading = true;\n      request.get(\"/permission\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false;\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        // 更新\n        request.put(\"/permission\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      } else {\n        // 新增\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n        let user = JSON.parse(userStr);\n        this.form.author = user.nickName;\n        request.post(\"/permission\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/permission/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); // 删除之后重新加载表格的数据\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      // 改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6FA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IADJ;MAELC,IAAI,EAAE,EAFD;MAGLC,aAAa,EAAE,KAHV;MAILC,MAAM,EAAE,EAJH;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,EANL;MAOLC,KAAK,EAAE,CAPF;MAQLC,SAAS,EAAE;IARN,CAAP;EAUD,CAdY;;EAebC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAjBY;;EAkBbC,OAAO,EAAE;IACPD,IAAI,GAAG;MACL,KAAKT,OAAL,GAAe,IAAf;MACAJ,OAAO,CAACe,GAAR,CAAY,aAAZ,EAA2B;QACzBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKT,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADiB,CAA3B,EAMGW,IANH,CAMQC,GAAE,IAAK;QACb,KAAKf,OAAL,GAAe,KAAf;QACA,KAAKO,SAAL,GAAiBQ,GAAG,CAAChB,IAAJ,CAASiB,OAA1B;QACA,KAAKV,KAAL,GAAaS,GAAG,CAAChB,IAAJ,CAASO,KAAtB;MACD,CAVD;IAWD,CAdM;;IAePW,GAAG,GAAG;MACJ,KAAKf,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CAlBM;;IAmBPiB,IAAI,GAAG;MAEL,IAAI,KAAKjB,IAAL,CAAUkB,EAAd,EAAkB;QAAG;QACnBvB,OAAO,CAACwB,GAAR,CAAY,aAAZ,EAA2B,KAAKnB,IAAhC,EAAsCa,IAAtC,CAA2CC,GAAE,IAAK;UAChDM,OAAO,CAACC,GAAR,CAAYP,GAAZ;;UACA,IAAIA,GAAG,CAACQ,IAAJ,KAAa,GAAjB,EAAsB;YACpB,KAAKC,QAAL,CAAc;cACZC,IAAI,EAAE,SADM;cAEZC,OAAO,EAAE;YAFG,CA