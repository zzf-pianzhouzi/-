{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 10,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"http://localhost:9090/user\", {\n        pageNum: this.currentPage,\n        pageSize: this.pageSize,\n        search: this.search\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      request.post(\"http://localhost:9090/user\", this.form).then(res => {\n        console.log(res);\n      });\n    },\n\n    handleEdit() {},\n\n    handleDelete() {},\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {}\n\n  }\n};","map":{"version":3,"mappings":"AAqGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,aAAa,EAAC,KAFT;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAC,EAND;MAOPC,SAAS,EAAC;IAPH,CAAP;EAaH,CApBc;;EAqBb;EACAC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAxBY;;EAyBbC,OAAO,EAAE;IACPD,IAAI,GAAE;MACJZ,OAAO,CAACc,GAAR,CAAY,4BAAZ,EAAyC;QACvCC,OAAO,EAAE,KAAKR,WADyB;QAEvCC,QAAQ,EAAE,KAAKA,QAFwB;QAGvCF,MAAM,EAAC,KAAKA;MAH2B,CAAzC,EAIGU,IAJH,CAIQC,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKP,SAAL,GAAeO,GAAG,CAACd,IAAJ,CAASiB,OAAxB;MACD,CAPD;IAQD,CAVM;;IAWPC,GAAG,GAAE;MACH,KAAKhB,aAAL,GAAmB,IAAnB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CAdM;;IAePkB,IAAI,GAAE;MACJtB,OAAO,CAACuB,IAAR,CAAa,4BAAb,EAA0C,KAAKnB,IAA/C,EAAqDY,IAArD,CAA0DC,GAAE,IAAI;QAC9DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACD,CAFD;IAGD,CAnBM;;IAoBPO,UAAU,GAAE,CAEX,CAtBM;;IAuBPC,YAAY,GAAE,CAEb,CAzBM;;IA0BPC,gBAAgB,GAAE,CAEjB,CA5BM;;IA6BPC,mBAAmB,GAAE,CAEpB;;EA/BM;AAzBI,CAAf","names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","created","load","methods","get","pageNum","then","res","console","log","records","add","save","post","handleEdit","handleDelete","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["D:\\Desktop\\springboot+vue\\springboot-vue-demo\\vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n<!--功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n<!--搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"输入关键字\" style=\"width:20%\"/>\n      <el-button type=\"primary\" style=\"margin-left: 5px\">查询</el-button>\n    </div>\n\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\n      <el-table-column\n          prop=\"id\"\n          label=\"ID\"\n          sortable\n          width=\"0\" />\n      <el-table-column\n          prop=\"username\"\n          label=\"用户名\"\n          width=\"0\" />\n      <el-table-column\n          prop=\"nickName\"\n          label=\"昵称\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"age\"\n          label=\"年龄\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"sex\"\n          label=\"性别\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"address\"\n          label=\"地址\"\n          width=\"0\"/>\n      <el-table-column\n          fixed=\"right\"\n          label=\"操作\"\n          width=\"200px\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\"  @click=\"handleEdit(scope.$index,scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.$index,scope.row)\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n      v-model:currentPage=\"currentPage\"\n      v-model:page-size=\"pageSize\"\n      :page-sizes=\"[5, 10, 20]\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      />\n\n      <el-dialog v-model=\"dialogVisible\" title=\"添加\" width=\"30%\">\n        <el-form :model=\"form\" label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio-group v-model=\"form.sex\" class=\"ml-4\">\n              <el-radio label=\"男\" size=\"large\">男</el-radio>\n              <el-radio label=\"女\" size=\"large\">女</el-radio>\n            </el-radio-group>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"/>\n          </el-form-item>\n        </el-form>\n        <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确定</el-button>\n      </span>\n        </template>\n      </el-dialog>\n\n</div>\n  </div>\n</template>\n\n<script>\n\n\nimport request from \"@/utils/request\";\n\nexport default {\n\n  name: 'HomeView',\n  components: {\n\n  },\n  data(){\n    return {\n      form:{},\n      dialogVisible:false,\n      search:'',\n      currentPage: 1,\n      pageSize: 10,\n      total:10,\n    tableData:[\n\n\n    ]\n\n  }\n},\n  //页面加载时调用\n  created() {\n    this.load()\n  },\n  methods: {\n    load(){\n      request.get(\"http://localhost:9090/user\",{\n        pageNum: this.currentPage,\n        pageSize: this.pageSize,\n        search:this.search\n      }).then(res=>{\n        console.log(res)\n        this.tableData=res.data.records\n      })\n    },\n    add(){\n      this.dialogVisible=true\n      this.form = {}\n    },\n    save(){\n      request.post(\"http://localhost:9090/user\",this.form).then(res =>{\n        console.log(res)\n      })\n    },\n    handleEdit(){\n\n    },\n    handleDelete(){\n\n    },\n    handleSizeChange(){\n\n    },\n    handleCurrentChange(){\n\n    },\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}