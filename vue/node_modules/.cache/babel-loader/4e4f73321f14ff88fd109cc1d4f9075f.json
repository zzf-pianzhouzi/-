{"ast":null,"code":"import E from 'wangeditor';\nimport request from \"@/utils/request\";\nlet editor;\nexport default {\n  name: 'stuNews',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      vis: false,\n      detail: {}\n    };\n  },\n\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    details(row) {\n      this.detail = row;\n      this.vis = true;\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.cover = res.data;\n    },\n\n    load() {\n      this.loading = true;\n      request.get(\"http://localhost:9090/news/getAllNews\").then(res => {\n        this.loading = false;\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      // 改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgFA,OAAOA,CAAP,MAAc,YAAd;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,IAAIC,MAAJ;AAEA,eAAe;EACbC,IAAI,EAAE,SADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,OAAO,EAAE,IAFJ;MAGLC,IAAI,EAAE,EAHD;MAILC,aAAa,EAAE,KAJV;MAKLC,MAAM,EAAE,EALH;MAMLC,WAAW,EAAE,CANR;MAOLC,QAAQ,EAAE,EAPL;MAQLC,KAAK,EAAE,CARF;MASLC,SAAS,EAAE,EATN;MAULC,GAAG,EAAE,KAVA;MAWLC,MAAM,EAAE;IAXH,CAAP;EAaD,CAjBY;;EAkBbC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKd,IAAL,GAAYe,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CAtBY;;EAuBbC,OAAO,EAAE;IACPC,OAAO,CAACC,GAAD,EAAM;MACX,KAAKV,MAAL,GAAcU,GAAd;MACA,KAAKX,GAAL,GAAW,IAAX;IACD,CAJM;;IAKPY,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CA