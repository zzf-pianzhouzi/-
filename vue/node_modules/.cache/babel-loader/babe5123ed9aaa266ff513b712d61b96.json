{"ast":null,"code":"import request from \"@/utils/request\"; //时间数据的存放\n\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      dialogVisible: false,\n      dialogVisible2: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 5,\n      total: 0,\n      params: {\n        date: '',\n        Time: ''\n      },\n\n      /*params: {\r\n        startTime: '',\r\n        endTime: '',\r\n        date: '',\r\n      },*/\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    /*dateFormat2(picker){\r\n      // this.params.startTime=picker[0]\r\n      // this.params.endTime=picker[1]\r\n      //类型是type=\"daterange\"\r\n      this.params.startTime=picker[0].toString\r\n      this.params.endTime=picker[1].toString\r\n    },*/\n    dateFormat(picker) {\n      // this.params.startTime=picker[0]\n      // this.params.endTime=picker[1]\n      //类型是type=\"daterange\"\n      this.params.date = date; // this.params.Time=picker\n\n      console.log(this.params.date);\n    },\n\n    formatDate(row, column) {\n      // 获取单元格数据\n      let data = row[column.property];\n\n      if (data == null) {\n        return null;\n      }\n\n      let dt = new Date(data);\n      return dt.getFullYear() + '-' + (dt.getMonth() + 1) + '-' + dt.getDate();\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    changeDate(val) {\n      this.$forceUpdate();\n      this.value = val;\n    },\n\n    load() {\n      request.get(\"http://localhost:9090/stu/queryStuAllAcidMsg\", {\n        params: {\n          stuId: this.user.stuId\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    input() {\n      this.$forceUpdate();\n    },\n\n    query() {\n      request.get(\"http://localhost:9090/stu/queryStuAllAcidMsg\", {\n        params: {\n          stuId: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    querybydate() {\n      request.get(\"http://localhost:9090/stu/queryStuDateAcidMsg\", {\n        params: {\n          // stuId:this.search,\n          date: this.params.Time\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    save() {\n      //上传图片\n      request.put(\"http://localhost:9090/stu\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数据\n\n        this.dialogVisible = false; //关闭弹窗\n      });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      /*    this.$nextTick(()=>{\r\n            this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n          })*/\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/stu/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //重新加载删除之后表格的数据\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+HA,OAAOA,OAAP,MAAoB,iBAApB,C,CAGA;;AAIA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,aAAa,EAAC,KAHT;MAILC,cAAc,EAAC,KAJV;MAKLC,MAAM,EAAC,EALF;MAMLC,WAAW,EAAE,CANR;MAOLC,QAAQ,EAAE,CAPL;MAQLC,KAAK,EAAC,CARD;MASLC,MAAM,EAAE;QACNC,IAAI,EAAE,EADA;QAENC,IAAI,EAAC;MAFC,CATH;;MAaL;;;;;MAKAC,SAAS,EAAC;IAlBL,CAAP;EAuBD,CA9BY;;EAgCb;EACAC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKf,IAAL,GAAYgB,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CArCY;;EAsCbC,OAAO,EAAE;IACP;;;;;;;IAOAC,UAAU,CAACC,MAAD,EAAQ;MAChB;MACA;MACA;MACA,KAAKb,MAAL,CAAYC,IAAZ,GAAiBA,IAAjB,CAJgB,CAKhB;;MACAa,OAAO,CAACC,GAAR,CAAY,KAAKf,MAAL,CAAYC,IAAxB;IACD,CAfM;;IAgBPe,UAAU,CAACC,GAAD,EAAMC,MAAN,EAAc;MACtB;MACA,IAAI3B,IAAG,GAAI0B,GAAG,CAACC,MAAM,CAACC,QAAR,CAAd;;MACA,IAAG5B,IAAG,IAAK,IAAX,EAAiB;QACf,OAAO,IAAP;MACF;;MACA,IAAI6B,EAAC,GAAI,IAAIC,IAAJ,CAAS9B,IAAT,CAAT;MACA,OAAO6B,EAAE,CAACE,WAAH,KAAmB,GAAnB,IAA0BF,EAAE,CAACG,QAAH,KAAgB,CAA1C,IAA+C,GAA/C,GAAqDH,EAAE,CAACI,OAAH,EAA5D;IACD,CAxBM;;IAyBPC,kBAAkB,CAACC,GAAD,EAAM;MACtBZ,OAAO,CAACC,GAAR,CAAYW,GAAZ;MACA,KAAKjC,IAAL,CAAUkC,GAAV,GAAgBD,GAAG,CAACnC,IAApB;IACD,CA5BM;;IA6BPqC,UAAU,CAACC,GAAD,EAAM;MACd,KAAKC,YAAL;MACA,KAAKC,KAAL,GAAYF,GAAZ;IACD,CAhCM;;IAiCPnB,IAAI,GAAE;MACJtB,OAAO,CAAC4C,GAAR,CAAY,8CAAZ,EAA2D;QACzDhC,MAAM,EAAC;UACLiC,KAAK,EAAC,KAAKzC,IAAL,CAAUyC;QADX;MADkD,CAA3D,EAIGC,IAJH,CAIQR,GAAG,IAAE;QACXZ,OAAO,CAACC,GAAR,CAAYW,GAAZ;QACA,KAAKvB,SAAL,GAAeuB,GAAG,CAACnC,IAAnB;QACA,KAAKQ,KAAL,GAAW2B,GAAG,CAACnC,IAAJ,CAAS4C,MAApB;MACD,CARD;IASD,CA3CM;;IA6CPC,KAAK,GAAG;MACN,KAAKN,YAAL;IACD,CA/CM;;IAgDPO,KAAK,GAAE;MACLjD,OAAO,CAAC4C,GAAR,CAAY,8CAAZ,EAA2D;QACzDhC,MAAM,EAAC;UACLiC,KAAK,EAAC,KAAKrC;QADN;MADkD,CAA3D,EAIGsC,IAJH,CAIQR,GAAG,IAAE;QACXZ,OAAO,CAACC,GAAR,CAAYW,GAAZ;QACA,KAAKvB,SAAL,GAAeuB,GAAG,CAACnC,IAAnB;QACA,KAAKQ,KAAL,GAAW2B,GAAG,CAACnC,IAAJ,CAAS4C,MAApB;MACD,CARD;IASD,CA1DM;;IA2DPG,WAAW,GA