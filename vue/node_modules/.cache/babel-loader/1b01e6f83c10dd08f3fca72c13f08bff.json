load(){\r\n      console.log(\"stuId\",this.user.stuId)\r\n      request.get(\"http://localhost:9090/stu/queryStuMsg\",{\r\n        params:{\r\n          stuId:this.user.stuId,\r\n        }\r\n      }).then(res=>{\r\n        console.log(\"loadres\",res)\r\n        this.tableData=res.data\r\n        this.total=res.data.length\r\n      })\r\n    },\r\n\r\n    input() {\r\n      this.$forceUpdate();\r\n    },\r\n\r\n    updatePhone(){\r\n      console.log(\"form\",this.form.phone)\r\n      request.get(\"http://localhost:9090/stu/changePho\",{\r\n        params:{\r\n          stuId:this.user.stuId,\r\n          phone:this.form.phone,\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"修改成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n        this.load()  //刷新表格的数据\r\n        this.dialogVisible2 = false //关闭弹窗\r\n      })\r\n    },\r\n    updatePassword(){\r\n      console.log(\"form\",this.form.pwd)\r\n      request.get(\"http://localhost:9090/stu/changePwd\",{\r\n        params:{\r\n          stuId:this.user.stuId,\r\n          pwd:this.form.pwd,\r\n        }\r\n      }).then(res=>{\r\n        console.log(res)\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"修改成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n        this.load()  //刷新表格的数据\r\n        this.dialogVisible3 = false //关闭弹窗\r\n      })\r\n    },\r\n    handleEdit(row){\r\n      this.form=JSON.parse(JSON.stringify(row))\r\n      this.dialogVisible=true\r\n    },\r\n    handleEdit2(){\r\n      this.dialogVisible2=true\r\n      this.form = {}\r\n      this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n    },\r\n    handleEdit3(){\r\n      this.dialogVisible3=true\r\n      this.form = {}\r\n      this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n    },\r\n    handleSizeChange(){       //改变当前每页的个数\r\n      this.load()\r\n    },\r\n    handleCurrentChange(){    //改变当前页面\r\n      this.load()\r\n    },\r\n\r\n  }\r\n}\r\n\r\n\r\n\r\n\r\n</script>"]},"metadata":{},"sourceType":"module"}