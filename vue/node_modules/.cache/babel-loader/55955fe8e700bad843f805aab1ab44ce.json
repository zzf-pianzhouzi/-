{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"http://localhost:9090/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put();\n      }\n\n      request.post(\"http://localhost:9090/user\", this.form).then(res => {\n        console.log(res);\n      });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete() {},\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {}\n\n  }\n};","map":{"version":3,"mappings":"AAqGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,aAAa,EAAC,KAFT;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAC,CAND;MAOPC,SAAS,EAAC;IAPH,CAAP;EAaH,CApBc;;EAqBb;EACAC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAxBY;;EAyBbC,OAAO,EAAE;IACPD,IAAI,GAAE;MACJZ,OAAO,CAACc,GAAR,CAAY,4BAAZ,EAAyC;QACvCC,MAAM,EAAC;UACLC,OAAO,EAAE,KAAKT,WADT;UAELC,QAAQ,EAAE,KAAKA,QAFV;UAGLF,MAAM,EAAC,KAAKA;QAHP;MADgC,CAAzC,EAOGW,IAPH,CAOQC,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKR,SAAL,GAAeQ,GAAG,CAACf,IAAJ,CAASkB,OAAxB;QACA,KAAKZ,KAAL,GAAWS,GAAG,CAACf,IAAJ,CAASM,KAApB;MACD,CAXD;IAYD,CAdM;;IAePa,GAAG,GAAE;MACH,