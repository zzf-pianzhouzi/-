{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 1,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let stuStr = sessionStorage.getItem(\"stu\") || \"{}\";\n    this.stu = JSON.parse(stuStr);\n    this.load();\n  },\n\n  methods: {\n    /*queryAllMatch() {\r\n      this.$axios\r\n          .get(\"/http://localhost:9090/stu/1/1\")\r\n          .then((response) => (this.tableData = response.data))\r\n          .catch(function (error) {\r\n            console.log(error);\r\n          });\r\n    },*/\n    load() {\n      this.$axios.get(\"/http://localhost:9090/stu/1/1\").then(response => this.tableData = response.data).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    /*load(){\r\n      request.get(\"http://localhost:9090/stu/1/1\"\r\n        /!*params:{\r\n          currPage: this.currentPage,\r\n          pageSize: this.pageSize,\r\n        }*!/\r\n        ).then(res=>{\r\n        console.log(res)\r\n        this.tableData=res.data.records\r\n        this.total=res.data.total\r\n      })\r\n    },*/\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n      this.$refs['upload'].clearFiles(); //清除历史文件列表\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"http://localhost:9090/stu\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else