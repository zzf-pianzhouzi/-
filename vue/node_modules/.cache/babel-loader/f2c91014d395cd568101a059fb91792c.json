{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"http://localhost:9090/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"http://localhost:9090/user\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else //新增\n        request.post(\"http://localhost:9090/user\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/user/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,aAAa,EAAC,KAFT;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAC,CAND;MAOPC,SAAS,EAAC;IAPH,CAAP;EAaH,CApBc;;EAqBb;EACAC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAxBY;;EAyBbC,OAAO,EAAE;IACPD,IAAI,GAAE;MACJZ,OAAO,CAACc,GAAR,CAAY,4BAAZ,EAAyC;QACvCC,MAAM,EAAC;UACLC,OAAO,EAAE,KAAKT,WADT;UAELC,QAAQ,EAAE,KAAKA,QAFV;UAGLF,MAAM,EAAC,KAAKA;QAHP;MADgC,CAAzC,EAOGW,IAPH,CAOQC,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKR,SAAL,GAAeQ,GAAG,CAACf,IAAJ,CAASkB,OAAxB;QACA,KAAKZ,KAAL,GAAWS,GAAG,CAACf,IAAJ,CAASM,KAApB;MACD,CAXD;IAYD,CAdM;;IAePa,GAAG,GAAE;MACH,KAAKjB,aAAL,GAAmB,IAAnB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CAlBM;;IAmBPmB,IAAI,GAAE;MACJ,IAAG,KAAKnB,IAAL,CAAUoB,EAAb,EAAgB;QAAG;QACjBxB,OAAO,CAACyB,GAAR,CAAY,4BAAZ,EAAyC,KAAKrB,IAA9C,EAAoDa,IAApD,CAAyDC,GAAE,IAAK;UAC9DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAIA,GAAG,CAACQ,IAAJ,KAAa,GAAjB,EAAsB;YACpB,KAAKC,QAAL,CAAc;cACZC,IAAI,EAAE,SADM;cAEZC,OAAO,EAAE;YAFG,CAAd;UAIF,CALA,MAKO;YACL,KAAKF,QAAL,CAAc;cACZC,IAAI,EAAE,OADM;cAEZC,OAAO,EAAEX,GAAG,CAACY;YAFD,CAAd;UAIF;;UACA,KAAKlB,IAAL,GAb8D,CAajD;;UACb,KAAKP,aAAL,GAAqB,KAArB,CAd8D,CAcnC;QAC5B,CAfD;MAgBD,CAjBD,MAiBkB;QAClBL,OAAO,CAAC+B,IAAR,CAAa,4BAAb,EAA0C,KAAK3B,IAA/C,EAAqDa,IAArD,CAA0DC,GAAE,IAAI;UAC9DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAIA,GAAG,CAACQ,IAAJ,KAAa,GAAjB,EAAsB;YACpB,KAAKC,QAAL,CAAc;cACZC,IAAI,EAAE,SADM;cAEZC,OAAO,EAAE;YAFG,CAAd;UAIF,CALA,MAKO;YACL,KAAKF,QAAL,CAAc;cACZC,IAAI,EAAE,OADM;cAEZC,OAAO,EAAEX,GAAG,CAACY;YAFD,CAAd;UAIF;;UACA,KAAKlB,IAAL,GAb8D,CAajD;;UACb,KAAKP,aAAL,GAAqB,KAArB,CAd8D,CAcnC;QAC5B,CAfD;IAgBD,CAtDM;;IAuDP2B,UAAU,CAACC,GAAD,EAAK;MACb;MACA,KAAK7B,IAAL,GAAU8B,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAV;MACA,KAAK5B,aAAL,GAAmB,IAAnB;IACD,CA3DM;;IA4DPgC,YAAY,CAACb,EAAD,EAAI;MACdL,OAAO,CAACC,GAAR,CAAYI,EAAZ;MACAxB,OAAO,CAACsC,MAAR,CAAe,gCAAgCd,EAA/C,EAAmDP,IAAnD,CAAwDC,GAAG,IAAE;QAC3D,IAAIA,GAAG,CAACQ,IAAJ,KAAa,GAAjB,EAAsB;UACpB,KAAKC,QAAL,CAAc;YACZC,IAAI,EAAE,SADM;YAEZC,OAAO,EAAE;UAFG,CAAd;QAIF,CALA,MAKO;UACL,KAAKF,QAAL,CAAc;YACZC,IAAI,EAAE,OADM;YAEZC,OAAO,EAAEX,GAAG,CAACY;UAFD,CAAd;QAIF;MACD,CAZD;IAaD,CA3EM;;IA4EPS,gBAAgB,GAAE;MAAQ;MACtB,KAAK3B,IAAL;IACH,CA9EM;;IA+EP4B,mBAAmB,GAAE;MAAK;MACtB,KAAK5B,IAAL;IACH;;EAjFM;AAzBI,CAAf","names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","created","load","methods","get","params","pageNum","then","res","console","log","records","add","save","id","put","code","$message","type","message","msg","post","handleEdit","row","JSON","parse","stringify","handleDelete","delete","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["D:\\Desktop\\springboot+vue\\springboot-vue-demo\\vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n<!--功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n<!--搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"输入关键字\" style=\"width:20%\" clearable/>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\n      <el-table-column\n          prop=\"id\"\n          label=\"ID\"\n          sortable\n          width=\"0\" />\n      <el-table-column\n          prop=\"username\"\n          label=\"用户名\"\n          width=\"0\" />\n      <el-table-column\n          prop=\"nickName\"\n          label=\"昵称\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"age\"\n          label=\"年龄\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"sex\"\n          label=\"性别\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"address\"\n          label=\"地址\"\n          width=\"0\"/>\n      <el-table-column\n          fixed=\"right\"\n          label=\"操作\"\n          width=\"200px\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\"  @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n      v-model:currentPage=\"currentPage\"\n      v-model:page-size=\"pageSize\"\n      :page-sizes=\"[5, 10, 20]\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      />\n\n      <el-dialog v-model=\"dialogVisible\" title=\"添加\" width=\"30%\">\n        <el-form :model=\"form\" label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio-group v-model=\"form.sex\" class=\"ml-4\">\n              <el-radio label=\"男\" size=\"large\">男</el-radio>\n              <el-radio label=\"女\" size=\"large\">女</el-radio>\n            </el-radio-group>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"/>\n          </el-form-item>\n        </el-form>\n        <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确定</el-button>\n      </span>\n        </template>\n      </el-dialog>\n\n</div>\n  </div>\n</template>\n\n<script>\n\n\nimport request from \"@/utils/request\";\n\nexport default {\n\n  name: 'HomeView',\n  components: {\n\n  },\n  data(){\n    return {\n      form:{},\n      dialogVisible:false,\n      search:'',\n      currentPage: 1,\n      pageSize: 10,\n      total:0,\n    tableData:[\n\n\n    ]\n\n  }\n},\n  //页面加载时调用\n  created() {\n    this.load()\n  },\n  methods: {\n    load(){\n      request.get(\"http://localhost:9090/user\",{\n        params:{\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search:this.search\n        }\n\n      }).then(res=>{\n        console.log(res)\n        this.tableData=res.data.records\n        this.total=res.data.total\n      })\n    },\n    add(){\n      this.dialogVisible=true\n      this.form = {}\n    },\n    save(){\n      if(this.form.id){  //更新\n        request.put(\"http://localhost:9090/user\",this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load()  //刷新表格的数据\n          this.dialogVisible = false //关闭弹窗\n        })\n      }else             //新增\n      request.post(\"http://localhost:9090/user\",this.form).then(res =>{\n        console.log(res)\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  //刷新表格的数据\n        this.dialogVisible = false //关闭弹窗\n      })\n    },\n    handleEdit(row){\n      //表单深拷贝\n      this.form=JSON.parse(JSON.stringify(row))\n      this.dialogVisible=true\n    },\n    handleDelete(id){\n      console.log(id)\n      request.delete(\"http://localhost:9090/user/\" + id).then(res=>{\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n      })\n    },\n    handleSizeChange(){       //改变当前每页的个数\n        this.load()\n    },\n    handleCurrentChange(){    //改变当前页面\n        this.load()\n    },\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}