{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      search: '',\n      currentPage: 1,\n      pageSize: 20,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    formatDate(row, column) {\n      // 获取单元格数据\n      let data = row[column.property];\n\n      if (data == null) {\n        return null;\n      }\n\n      let dt = new Date(data);\n      return dt.getFullYear() + '-' + (dt.getMonth() + 1 < 10 ? '0' + (dt.getMonth() + 1) : dt.getMonth() + 1) + '-' + (dt.getDate() < 10 ? '0' + dt.getDate() : dt.getDate()) + ' ' + (dt.getHours() < 10 ? '0' + dt.getHours() : dt.getHours()) + ':' + (dt.getMinutes() < 10 ? '0' + dt.getMinutes() : dt.getMinutes()) + ':' + (dt.getSeconds() < 10 ? '0' + dt.getSeconds() : dt.getSeconds());\n    },\n\n    load() {\n      request.post(\"http://localhost:9090/common/allTchAcidMsg\", {}).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    query() {\n      console.log(\"this.search\", this.search);\n      request.get(\"http://localhost:9090/tch/myAcidMsg\", {\n        params: {\n          stuId: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    handleCheck(id) {\n      console.log(\"id\", id);\n      request.put(\"http://localhost:9090/tch/check/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"核验成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表单\n      });\n    },\n\n    handleCheck2(id) {\n      console.log(\"id\", id);\n      request.put(\"http://localhost:9090/tch/checked/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"核验成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表单\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmFA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAC,CAND;MAOLC,SAAS,EAAC;IAPL,CAAP;EAaD,CApBY;;EAqBb;EACAC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKV,IAAL,GAAYW,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CA1BY;;EA2BbC,OAAO,EAAE;IACPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKf,IAAL,CAAUkB,GAAV,GAAgBH,GAAG,CAACjB,IAApB;IACD,CAJM;;IAKPqB,UAAU,CAACC,GAAD,EAAMC,MAAN,EAAc;MACtB;MACA,IAAIvB,IAAG,GAAIsB,GAAG,CAACC,MAAM,CAACC,QAAR,CAAd;;MACA,IAAGxB,IAAG,IAAK,IAAX,EAAiB;QACf,OAAO,IAAP;MACF;;MACA,IAAIyB,EAAC,GAAI,IAAIC,IAAJ,CAAS1B,IAAT,CAAT;MACA,OAAOyB,EAAE,CAACE,WAAH,KAAmB,GAAnB,IAA6BF,EAAE,CAACG,QAAH,KAAgB,CAAjB,GAAsB,EAAtB,GAA4B,OAAOH,EAAE,CAACG,QAAH,KAAgB,CAAvB,CAA5B,GAA0DH,EAAE,CAACG,QAAH,KAAgB,CAAtG,IAA4G,GAA5G,IAEFH,EAAE,CAACI,OAAH,KAAe,EAAf,GAAqB,MAAMJ,EAAE,CAACI,OAAH,EAA3B,GAA2CJ,EAAE,CAACI,OAAH,EAFzC,IAEyD,GAFzD,IAIFJ,EAAE,CAACK,QAAH,KAAgB,EAAhB,GAAsB,MAAML,EAAE,CAACK,QAAH,E