{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Password\",\n\n  data() {\n    return {\n      form: {\n        password: '',\n        newPass: '',\n        confirmPass: ''\n      },\n      rules: {\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }],\n        newPass: [{\n          required: true,\n          message: '请输入新密码',\n          trigger: 'blur'\n        }],\n        confirmPass: [{\n          required: true,\n          message: '请输入确认新密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    changePsss() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (!this.form.newPass === this.form.confirmPass) {\n            this.$message.error('2次输入新密码必须一致');\n            return;\n          }\n\n          let user = JSON.parse(sessionStorage.getItem(\"user\"));\n          this.form.userId = user.id;\n          request.put(\"/user/pass\", this.form).then(res => {\n            if (res.code === '0') {\n              this.$message.success('修改成功');\n              this.$router.push(\"/login\");\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAuBA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACNC,IAAI,EAAE;QACJC,QAAQ,EAAE,EADN;QAEJC,OAAO,EAAE,EAFL;QAGJC,WAAW,EAAE;MAHT,CADA;MAMLC,KAAK,EAAE;QACLH,QAAQ,EAAE,CACR;UAACI,QAAQ,EAAE,IAAX;UAAiBC,OAAO,EAAE,OAA1B;UAAmCC,OAAO,EAAE;QAA5C,CADQ,CADL;QAILL,OAAO,EAAE,CACP;UAACG,QAAQ,EAAE,IAAX;UAAiBC,OAAO,EAAE,QAA1B;UAAoCC,OAAO,EAAE;QAA7C,CADO,CAJJ;QAOLJ,WAAW,EAAE,CACX;UAACE,QAAQ,EAAE,IAAX;UAAiBC,OAAO,EAAE,UAA1B;UAAsCC,OAAO,EAAE;QAA/C,CADW;MAPR;IANF,CAAP;EAkBD,CArBY;;EAsBbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,KAAKC,KAAL,CAAW,MAAX,EAAmBC,QAAnB,CAA6BC,KAAD,IAAW;QACrC,IAAIA,KAA