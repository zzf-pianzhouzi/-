{"ast":null,"code":"import request from \"@/utils/request\"; //时间数据的存放\n\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      dialogVisible: false,\n      dialogVisible2: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 5,\n      total: 0,\n      params: {\n        startTime: '',\n        endTime: '',\n        date: ''\n      },\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    dateFormat(picker) {\n      // this.params.startTime=picker[0]\n      // this.params.endTime=picker[1]\n      //类型是type=\"daterange\"\n      this.params.startTime = picker[0].toString;\n      this.params.endTime = picker[1].toString;\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    changeDate(val) {\n      this.$forceUpdate();\n      this.value = val;\n    },\n\n    load() {\n      request.get(\"http://localhost:9090/stu/queryStuAllAcidMsg\", {\n        params: {\n          stuId: this.user.stuId\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    input() {\n      this.$forceUpdate();\n    },\n\n    query() {\n      request.get(\"http://localhost:9090/stu/queryStuAllAcidMsg\", {\n        params: {\n          stuId: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    save() {\n      //上传图片\n      request.put(\"http://localhost:9090/stu\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数据\n\n        this.dialogVisible = false; //关闭弹窗\n      });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      /*    this.$nextTick(()=>{\r\n            this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n          })*/\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/stu/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //重新加载删除之后表格的数据\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwHA,OAAOA,OAAP,MAAoB,iBAApB,C,CAGA;;AAIA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,aAAa,EAAC,KAHT;MAILC,cAAc,EAAC,KAJV;MAKLC,MAAM,EAAC,EALF;MAMLC,WAAW,EAAE,CANR;MAOLC,QAAQ,EAAE,CAPL;MAQLC,KAAK,EAAC,CARD;MASLC,MAAM,EAAE;QACNC,SAAS,EAAE,EADL;QAENC,OAAO,EAAE,EAFH;QAGNC,IAAI,EAAE;MAHA,CATH;MAcLC,SAAS,EAAC;IAdL,CAAP;EAmBD,CA1BY;;EA4Bb;EACAC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKhB,IAAL,GAAYiB,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CAjCY;;EAkCbC,OAAO,EAAE;IACPC,UAAU,CAACC,MAAD,EAAQ;MAChB;MACA;MACA;MACA,KAAKd,MAAL,CAAYC,SAAZ,GAAsBa,MAAM,CAAC,CAAD,CAAN,CAAUC,QAAhC;MACA,KAAKf,MAAL,CAAYE,OAAZ,GAAoBY,MAAM,CAAC,CAAD,CAAN,CAAUC,QAA9B;IACD,CAPM;;IAQPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKxB,IAAL,CAAU2B,GAAV,GAAgBH,GAAG,CAAC1B,IAApB;IACD,CAXM