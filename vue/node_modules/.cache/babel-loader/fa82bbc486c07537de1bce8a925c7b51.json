{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 1,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let stuStr = sessionStorage.getItem(\"stu\") || \"{}\";\n    this.stu = JSON.parse(stuStr);\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"http://localhost:9090/stu/1/1\", {\n        /*params:{\r\n          currPage: this.currentPage,\r\n          pageSize: this.pageSize,\r\n        }*/\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n      this.$refs['upload'].clearFiles(); //清除历史文件列表\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"http://localhost:9090/stu\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else //新增\n        request.post(\"http://localhost:9090/stu\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      this.$nextTick(() => {\n        this.$refs['upload'].clearFiles(); //清除历史文件列表\n      });\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/stu/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //重新加载删除之后表格的数据\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,aAAa,EAAC,KAHT;MAILC,MAAM,EAAC,EAJF;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,CANL;MAOLC,KAAK,EAAC,CAPD;MAQLC,SAAS,EAAC;IARL,CAAP;EAcD,CArBY;;EAsBb;EACAC,OAAO,GAAG;IACR,IAAIC,MAAK,GAAIC,cAAc,CAACC,OAAf,CAAuB,KAAvB,KAAiC,IAA9C;IACA,KAAKC,GAAL,GAAWC,IAAI,CAACC,KAAL,CAAWL,MAAX,CAAX;IACA,KAAKM,IAAL;EACD,CA3BY;;EA4BbC,OAAO,EAAE;IACPD,IAAI,GAAE;MACJnB,OAAO,CAACqB,GAAR,CAAY,+BAAZ,EAA4C;QAC1C;;;;MAD0C,CAA5C,EAMGC,IANH,CAMQC,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKZ,SAAL,GAAeY,GAAG,CAACpB,IAAJ,CAASuB,OAAxB;QACA,KAAKhB,KAAL,GAAWa,GAAG,CAACpB,IAAJ,CAASO,KAApB;MACD,CAVD;IAWD,CAbM;;IAcPiB,GAAG,GAAE;MACH,KAAKrB,aAAL,GAAmB,IAAnB;MACA,KAAKD,IAAL,GAAY,EAAZ;MACA,KAAKuB,KAAL,CAAW,QAAX,EAAqBC,UAArB,GAHG,CAGiC;IACrC,CAlBM;;IAmBPC,IAAI,GAAE;MACJ,IAAG,KAAKzB,IAAL,CAAU0B,EAAb,EAAgB;QAAG;QACjB/B,OAAO,CAACgC,GAAR,CAAY,2BAAZ,EAAwC,KAAK3B,IAA7C,EAAmDiB,IAAnD,CAAwDC,GAAE,IAAK;UAC7DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAIA,GAAG,CAACU,IAAJ,KAAa