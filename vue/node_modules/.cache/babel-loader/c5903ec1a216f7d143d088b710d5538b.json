\n      <el-table-column\n          prop=\"sex\"\n          label=\"性别\"\n          width=\"0\"/>\n      <el-table-column\n          prop=\"address\"\n          label=\"地址\"\n          width=\"0\"/>\n      <el-table-column\n          fixed=\"right\"\n          label=\"操作\"\n          width=\"200px\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\"  @click=\"handleEdit(scope.$index,scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.$index,scope.row)\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n      v-model:currentPage=\"currentPage\"\n      v-model:page-size=\"pageSize\"\n      :page-sizes=\"[5, 10, 20]\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      />\n</div>\n  </div>\n</template>\n\n<script>\n\n\nexport default {\n  name: 'HomeView',\n  components: {\n\n  },\n  data(){\n    return {\n      search:'',\n      currentPage: 1,\n      pageSize: 10,\n      total:10,\n    tableData:[\n        \n\n    ]\n\n  }\n},\n  methods: {\n    handleEdit(){\n\n    },\n    handleDelete(){\n\n    },\n    handleSizeChange(){\n\n    },\n    handleCurrentChange(){\n\n    },\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}