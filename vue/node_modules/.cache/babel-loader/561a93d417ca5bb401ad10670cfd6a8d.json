{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      search: '',\n      currentPage: 1,\n      pageSize: 2,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    load() {\n      request.get(\"http://localhost:9090/tch/allStuMsg\", {\n        params: {\n          classes: this.user.classes\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    query() {\n      request.get(\"http://localhost:9090/tch/query\", {\n        params: {\n          name: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    handleCheck(id) {\n      request.put(\"http://localhost:9090/tch/check/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"核验成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表单\n      });\n    },\n\n    handleCheck2(id) {\n      request.put(\"http://localhost:9090/tch/checked/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"核验成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表单\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgFA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,CALL;MAMLC,KAAK,EAAC,CAND;MAOLC,SAAS,EAAC;IAPL,CAAP;EAaD,CApBY;;EAqBb;EACAC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKV,IAAL,GAAYW,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CA1BY;;EA2BbC,OAAO,EAAE;IACPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKf,IAAL,CAAUkB,GAAV,GAAgBH,GAAG,CAACjB,IAApB;IACD,CAJM;;IAKPc,IAAI,GAAE;MACJjB,OAAO,CAACwB,GAAR,CAAY,qCAAZ,EAAkD;QAChDC,MAAM,EAAC;UACLC,OAAO,EAAC,KAAKtB,IAAL,CAAUsB;QADb;MADyC,CAAlD,EAIGC,IAJH,CAIQP,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAeU,GAAG,CAACjB,IAAnB;QACA,KAAKM,KAAL,GAAWW,GAAG,CAACjB,IAAJ,CAASyB,MAApB;MACD,CARD;IASD,CAfM;;IAgBPC,KAAK,GAAE;MACL7B,OAAO,CAACwB,GAAR,CAAY,iCAAZ,EAA8C;QAC5CC,MAAM,EAAC;UACLxB,IAAI,EAAC,KAAKK;QADL;MADqC,CAA9C,EAIGqB,IAJH,CAIQP,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAeU,GAAG,CAACjB,IAAnB;QACA,KAAKM,KAAL,GAAWW,GAAG,CAACjB,IAAJ,CAASyB,MAApB;MACD,CARD;IASD,CA1BM;;IA2BPE,WAAW,CAACC,EAAD,EAAI;MAEb/B,OAAO,CAACgC,GAAR,CAAY,qCAAqCD,EAAjD,EAAqDJ,IAArD,CAA0DP,GAAG,IAAE;QAC7DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;QACA,IAAIA,GAAG,CAACa,IAAJ,KAAa,GAAjB,EAAsB;UACpB,KAAKC,QAAL,CAAc;YACZC,IAAI,EAAE,SADM;YAEZC,OAAO,EAAE;UAFG,CAAd;QAIF,CALA,MAKO;UACL,KAAKF,QAAL,CAAc;YACZC,IAAI,EAAE,OADM;YAEZC,OAAO,EAAEhB,GAAG,CAACiB;UAFD,CAAd;QAIF;;QACA,KAAKpB,IAAL,GAb6D,CAahD;MACd,CAdD;IAiBD,CA9CM;;IA+CPqB,YAAY,CAACP,EAAD,EAAI;MAEd/B,OAAO,CAACgC,GAAR,CAAY,uCAAuCD,EAAnD,EAAuDJ,IAAvD,CAA4DP,GAAG,IAAE;QAC/DC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;QACA,IAAIA,GAAG,CAACa,IAAJ,KAAa,GAAjB,EAAsB;UACpB,KAAKC,QAAL,