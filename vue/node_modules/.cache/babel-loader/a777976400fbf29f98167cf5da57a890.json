{"ast":null,"code":"import E from 'wangeditor';\nimport request from \"@/utils/request\";\nlet editor;\nexport default {\n  name: 'stuNews',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      params: {\n        date: ''\n      },\n      total: 0,\n      tableData: [],\n      vis: false,\n      detail: {}\n    };\n  },\n\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    details(row) {\n      this.detail = row;\n      this.vis = true;\n    },\n\n    dateFormat(picker) {\n      // this.params.startTime=picker[0]\n      // this.params.endTime=picker[1]\n      //类型是type=\"daterange\"\n      this.params.date = picker; // this.params.Time=picker\n    },\n\n    formatDate(row, column) {\n      // 获取单元格数据\n      let data = row[column.property];\n\n      if (data == null) {\n        return null;\n      }\n\n      let dt = new Date(data);\n      return dt.getFullYear() + '-' + (dt.getMonth() + 1 < 10 ? '0' + (dt.getMonth() + 1) : dt.getMonth() + 1) + '-' + (dt.getDate() < 10 ? '0' + dt.getDate() : dt.getDate()) + ' ' + (dt.getHours() < 10 ? '0' + dt.getHours() : dt.getHours()) + ':' + (dt.getMinutes() < 10 ? '0' + dt.getMinutes() : dt.getMinutes()) + ':' + (dt.getSeconds() < 10 ? '0' + dt.getSeconds() : dt.getSeconds());\n    },\n\n    query() {\n      console.log(\"date\", this.params.date);\n      request.get(\"http://localhost:9090/news/queryDate\", {\n        params: {\n          date: this.params.date\n        }\n      }).then(res => {\n        console.log(res);\n        console.log(\"res.data.length\", res.data.length);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.cover = res.data;\n    },\n\n    load() {\n      this.loading = true;\n      request.get(\"http://localhost:9090/news/getAllNews\").then(res => {\n        this.loading = false;\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      // 改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwFA,OAAOA,CAAP,MAAc,YAAd;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,IAAIC,MAAJ;AAEA,eAAe;EACbC,IAAI,EAAE,SADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,OAAO,EAAE,IAFJ;MAGLC,IAAI,EAAE,EAHD;MAILC,aAAa,EAAE,KAJV;MAKLC,MAAM,EAAE,EALH;MAMLC,WAAW,EAAE,CANR;MAOLC,QAAQ,EAAE,EAPL;MAQLC,MAAM,EAAE;QACNC,IAAI,EAAE;MADA,CARH;MAWLC,KAAK,EAAE,CAXF;MAYLC,SAAS,EAAE,EAZN;MAaLC,GAAG,EAAE,KAbA;MAcLC,MAAM,EAAE;IAdH,CAAP;EAgBD,CApBY;;EAqBbC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKhB,IAAL,GAAYiB,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CAzBY;;EA0BbC,OAAO,EAAE;IACPC,OAAO,CAACC,GAAD,EAAM;MACX,KAAKV,MAAL,GAAcU,GAAd;MACA,KAAKX,GAAL,GAAW,IAAX;IACD,CAJM;;IAKPY,UAAU,CAACC,MAAD,EAAQ;MAChB;MACA;MACA;MACA,KAAKjB,MAAL,CAAYC,IAAZ,GAAiBgB,MAAjB,CAJgB,CAKhB;IACD,CAXM;;IAYPC,UAAU,CAACH,GAAD,EAAMI,MAAN,EAAc;MACtB;MACA,IAAI3B,IAAG,GAAIuB,GAAG,CAACI,MAAM,CAACC,QAAR,CAAd;;MACA,IAAG5B,IAAG,IAAK,IAAX,EAAiB;QACf,OAAO,IAAP;MACF;;MACA,IAAI6B,EAAC,GAAI,IAAIC,IAAJ,CAAS9B,IAAT,CAAT;MACA,OAAO6B,EAAE,CAACE,WAAH,KAAmB,GAAnB,IAA6BF,EAAE,CAACG,QAAH,KAAgB,CAAjB,GAAsB,EAAtB,GAA4B,OAAOH,EAAE,CAACG,QAAH,KAAgB,CAAvB,CAA5B,GAA0DH,EAAE,CAACG,QAAH,KAAgB,CAAtG,IAA4G,GAA5G,IAEFH,EAAE,CAACI,OAAH,KAAe,EAAf,GAAqB,MAAMJ,EAAE,CAACI,OAAH,EAA3B,GAA2CJ,EAAE,CAACI,OAAH,EAFzC,IAEyD,GAFzD,IAIFJ,EAAE,CAACK,QAAH,KAAgB,EAAhB,GAAsB,MAAML,EAAE,CAACK,QAAH,EAA5B,GAA6CL,EAAE,CAACK,QAAH,EAJ3C,IAI4D,GAJ5D,IAMFL,EAAE,CAACM,UAAH,KAAkB,EAAlB,GAAwB,MAAMN,EAAE,CAA