{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'Role',\n  components: {},\n\n  data() {\n    return {\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      permissions: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    handleChange(row) {\n      request.put(\"/role/changePermission\", row).then(res => {\n        if (res.code === '0') {\n          this.$message.success(\"更新成功\");\n\n          if (res.data) {\n            this.$router.push(\"/login\");\n          }\n        }\n      });\n    },\n\n    load() {\n      this.loading = true;\n      request.get(\"/role\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false;\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n      request.get(\"/permission/all\").then(res => {\n        this.permissions = res.data;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        // 更新\n        request.put(\"/role\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      } else {\n        // 新增\n        let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n        let user = JSON.parse(userStr);\n        this.form.author = user.nickName;\n        request.post(\"/role\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); // 刷新表格的数据\n\n          this.dialogVisible = false; // 关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/role/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); // 删除之后重新加载表格的数据\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      // 改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAuFA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IADJ;MAELC,IAAI,EAAE,EAFD;MAGLC,aAAa,EAAE,KAHV;MAILC,MAAM,EAAE,EAJH;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,EANL;MAOLC,KAAK,EAAE,CAPF;MAQLC,SAAS,EAAE,EARN;MASLC,WAAW,EAAE;IATR,CAAP;EAWD,CAfY;;EAgBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAlBY;;EAmBbC,OAAO,EAAE;IACPC,YAAY,CAACC,GAAD,EAAM;MAChBjB,OAAO,CAACkB,GAAR,CAAY,wBAAZ,EAAsCD,GAAtC,EAA2CE,IAA3C,CAAgDC,GAAE,IAAK;QACrD,IAAIA,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;UACpB,KAAKC,QAAL,CAAcC,OAAd,CAAsB,MAAtB;;UACA,IAAIH,GAAG,CAACjB,IAAR,EAAc;YACZ,KAAKqB,OAAL,CAAaC,IAAb,CAAkB,QAAlB;UACF;QACF;MACD,CAPD;IAQD,