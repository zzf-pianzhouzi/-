{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      search: '',\n      currentPage: 1,\n      pageSize: 5,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    load() {\n      request.get(\"http://localhost:9090/stu\", {\n        params: {\n          currPage: this.currentPage,\n          pageSize: this.pageSize,\n          name: \"\"\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    query() {\n      request.get(\"http://localhost:9090/stu/query\", {\n        params: {\n          name: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    save() {\n      //更新\n      request.put(\"http://localhost:9090/stu\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数据\n\n        this.dialogVisible = false; //关闭弹窗\n      });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      /*    this.$nextTick(()=>{\r\n            this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n          })*/\n    },\n\n    handleEdit2() {\n      this.dialogVisible2 = true;\n      this.form = {};\n      this.$refs['upload'].clearFiles(); //清除历史文件列表\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/stu/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //重新加载删除之后表格的数据\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,IAAI,EAAC,EAFA;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,CALL;MAMLC,KAAK,EAAC,CAND;MAOLC,SAAS,EAAC;IAPL,CAAP;EAaD,CApBY;;EAqBb;EACAC,OAAO,GAAG;IACR,IAAIC,OAAM,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAAhD;IACA,KAAKV,IAAL,GAAYW,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAZ;IACA,KAAKK,IAAL;EACD,CA1BY;;EA2BbC,OAAO,EAAE;IACPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKf,IAAL,CAAUkB,GAAV,GAAgBH,GAAG,CAACjB,IAApB;IACD,CAJM;;IAKPc,IAAI,GAAE;MACJjB,OAAO,CAACwB,GAAR,CAAY,2BAAZ,EAAwC;QACtCC,MAAM,EAAC;UACLC,QAAQ,EAAE,KAAKnB,WADV;UAELC,QAAQ,EAAE,KAAKA,QAFV;UAGLP,IAAI,EAAC;QAHA;MAD+B,CAAxC,EAMG0B,IANH,CAMQP,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAeU,GAAG,CAACjB,IAAnB;QACA,KAAKM,KAAL,GAAWW,GAAG,CAACjB,IAAJ,CAASyB,MAApB;MACD,CAVD;IAWD,CAjBM;;IAkBPC,KAAK,GAAE;MACL7B,OAAO,CAACwB,GAAR,CAAY,iCAAZ,EAA8C;QAC5CC,MAAM,EAAC;UACLxB,IAAI,EAAC,KAAKK;QADL;MADqC,CAA9C,EAIGqB,IAJH,CAIQP,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAeU,GAAG,CAACjB,IAAnB;QACA,KAAKM,KAAL,GAAWW,GAAG,CAACjB,IAAJ,CAASyB,MAApB;MACD,CARD;IASD,CA5BM;;IA6BPE,IAAI,GAAE;MACJ;MACE9B,OAAO,CAAC+B,GAAR,CAAY,2BAAZ,EAAwC,KAAK1B,IAA7C