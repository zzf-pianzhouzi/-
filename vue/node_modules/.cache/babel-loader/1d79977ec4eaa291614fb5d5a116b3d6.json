{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      stu: {},\n      form: {},\n      dialogVisible: false,\n      dialogVisible2: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 1,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  //页面加载时调用\n  created() {\n    let stuStr = sessionStorage.getItem(\"stu\") || \"{}\";\n    this.stu = JSON.parse(stuStr);\n    this.load();\n  },\n\n  methods: {\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.pic = res.data;\n    },\n\n    load() {\n      request.get(\"http://localhost:9090/stu\" + name, {\n        params: {\n          currPage: this.currentPage,\n          pageSize: this.pageSize,\n          name: \"\"\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.data.length;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n      this.$refs['upload'].clearFiles(); //清除历史文件列表\n    },\n\n    save() {\n      //更新\n      request.put(\"http://localhost:9090/stu\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数据\n\n        this.dialogVisible = false; //关闭弹窗\n      });\n    },\n\n    save2() {\n      //新增\n      request.post(\"http://localhost:9090/stu\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数据\n\n        this.dialogVisible2 = false; //关闭弹窗\n      });\n    },\n\n    handleEdit(row) {\n      //表单深拷贝\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n      /*    this.$nextTick(()=>{\r\n            this.$refs['upload'].clearFiles()   //清除历史文件列表\r\n          })*/\n    },\n\n    handleEdit2() {\n      this.dialogVisible2 = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"http://localhost:9090/stu/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //重新加载删除之后表格的数据\n      });\n    },\n\n    handleSizeChange() {\n      //改变当前每页的个数\n      this.load();\n    },\n\n    handleCurrentChange() {\n      //改变当前页面\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmJA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EAEbC,IAAI,EAAE,UAFO;EAGbC,UAAU,EAAE,EAHC;;EAMbC,IAAI,GAAE;IACJ,OAAO;MACLC,GAAG,EAAC,EADC;MAELC,IAAI,EAAC,EAFA;MAGLC,aAAa,EAAC,KAHT;MAILC,cAAc,EAAC,KAJV;MAKLC,MAAM,EAAC,EALF;MAMLC,WAAW,EAAE,CANR;MAOLC,QAAQ,EAAE,CAPL;MAQLC,KAAK,EAAC,CARD;MASLC,SAAS,EAAC;IATL,CAAP;EAeD,CAtBY;;EAuBb;EACAC,OAAO,GAAG;IACR,IAAIC,MAAK,GAAIC,cAAc,CAACC,OAAf,CAAuB,KAAvB,KAAiC,IAA9C;IACA,KAAKZ,GAAL,GAAWa,IAAI,CAACC,KAAL,CAAWJ,MAAX,CAAX;IACA,KAAKK,IAAL;EACD,CA5BY;;EA6BbC,OAAO,EAAE;IACPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACA,KAAKjB,IAAL,CAAUoB,GAAV,GAAgBH,GAAG,CAACnB,IAApB;IACD,CAJM;;IAKPgB,IAAI,GAAE;MACJnB,OAAO,CAAC0B,GAAR,CAAY,8BAA4BzB,IAAxC,EAA6C;QAC3C0B,MAAM,EAAC;UACLC,QAAQ,EAAE,KAAKnB,WADV;UAELC,QAAQ,EAAE,KAAKA,QAFV;UAGLT,IAAI,EAAC;QAHA;MADoC,CAA7C,EAMG4B,IANH,CAMQP,GAAG,IAAE;QACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKV,SAAL,GAAeU,GAAG,CAACnB,IAAnB;QACA,KAAKQ,KAAL,GAAWW,GAAG,CAACnB,IAAJ,CAAS